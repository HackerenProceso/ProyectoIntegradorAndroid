<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="650dp"
        android:layout_marginTop="32dp"
        android:layout_margin="16dp"
        android:orientation="vertical"
        android:background="@drawable/rounded_corners_background"
        android:padding="16dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="13dp"
            android:text="Nombre: "
            android:textSize="15sp"
            android:textColor="@android:color/black"
            android:paddingBottom="11dp" />

        <com.google.android.material.textfield.TextInputLayout
            android:layout_width="320dp"
            android:layout_height="wrap_content">

            <EditText
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:background="@drawable/shadow_producto"
                android:backgroundTint="@color/fondo_reclamo"
                android:hint="" />

        </com.google.android.material.textfield.TextInputLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="13dp"
            android:text="Apellido: "
            android:textSize="15sp"
            android:textColor="@android:color/black"
            android:paddingBottom="11dp" />

        <com.google.android.material.textfield.TextInputLayout
            android:layout_width="320dp"
            android:layout_height="wrap_content">

            <EditText
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:background="@drawable/shadow_producto"
                android:backgroundTint="@color/fondo_reclamo"
                android:hint="" />

        </com.google.android.material.textfield.TextInputLayout>


        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="13dp"
            android:text="DNI: "
            android:textSize="15sp"
            android:textColor="@android:color/black"
            android:paddingBottom="11dp" />

        <com.google.android.material.textfield.TextInputLayout
            android:layout_width="320dp"
            android:layout_height="wrap_content">

            <EditText
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:background="@drawable/shadow_producto"
                android:backgroundTint="@color/fondo_reclamo"
                android:hint="" />

        </com.google.android.material.textfield.TextInputLayout>


        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="13dp"
            android:text="Correo: "
            android:textSize="15sp"
            android:textColor="@android:color/black"
            android:paddingBottom="11dp" />

        <com.google.android.material.textfield.TextInputLayout
            android:layout_width="320dp"
            android:layout_height="wrap_content">

            <EditText
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:background="@drawable/shadow_producto"
                android:backgroundTint="@color/fondo_reclamo"
                android:hint="" />

        </com.google.android.material.textfield.TextInputLayout>



        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="13dp"
            android:text="*Campos obligatorios"
            android:textSize="13sp"
            android:textColor="@color/letra_roja"
            android:paddingBottom="11dp" />

        <TextView
            android:id="@+id/Rcelular"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="13dp"
            android:text="Celular: "
            android:textSize="15sp"
            android:textColor="@android:color/black"
            android:paddingBottom="11dp" />

        <com.google.android.material.textfield.TextInputLayout
            android:layout_width="320dp"
            android:layout_height="wrap_content">

            <EditText
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:background="@drawable/shadow_producto"
                android:backgroundTint="@color/fondo_reclamo"
                android:hint="" />

        </com.google.android.material.textfield.TextInputLayout>


        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="13dp"
            android:text="DirecciÃ³n: "
            android:textSize="15sp"
            android:textColor="@android:color/black"
            android:paddingBottom="11dp" />

        <com.google.android.material.textfield.TextInputLayout
            android:layout_width="320dp"
            android:layout_height="wrap_content">

            <EditText
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:background="@drawable/shadow_producto"
                android:backgroundTint="@color/fondo_reclamo"
                android:hint="" />

        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.button.MaterialButton
            android:id="@+id/btnContinuar02"
            android:layout_width="160dp"
            android:layout_height="60dp"
            android:layout_marginVertical="10dp"
            android:layout_marginHorizontal="160dp"
            android:backgroundTint="@color/black"
            android:text="Continuar"
            app:cornerRadius="10dp" />
    </LinearLayout>
    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/fragmentNav"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        app:layout_constraintBottom_toBottomOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>

